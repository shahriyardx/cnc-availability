    "import random\n",
    "\n",
    "def _random(nums, *args):\n",
    "    if isinstance(nums, list):\n",
    "        print(nums, 1)\n",
    "        return random.choice(nums)\n",
    "    print([nums, *args], 2)\n",
    "    return random.choice([nums, *args])\n",
    "\n",
    "_random([\"A\", \"B\", \"C\"])"
   ]
  }
